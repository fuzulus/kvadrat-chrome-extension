{
    "manifest_version": 3,
    "name": "Kvadrat",
    "version": "1.0",
    "description": "Check prices per square meter on ad sites such as Njuškalo. Currently supports: Njuškalo.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content/square_meter_njuskalo_list.js"],
            "matches": [
                "https://www.njuskalo.hr/prodaja-kuca*",
                "https://www.njuskalo.hr/prodaja-stanova*"
            ]
        }
    ]
}